    "babel-preset-es2015": "^6.9.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.3.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "webpack": "^1.13.1",
    "webpack-bundle-size-analyzer": "^2.0.2",
    "yargs": "^4.7.1"
  },
  "repository": {
    "type": "git",
